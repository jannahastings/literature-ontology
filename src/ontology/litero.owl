        <obo:IAO_0000115>A fictional work that is the length of a book.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">novel</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/LITERO_000016 -->

