        <obo:IAO_0000115>A generically dependent continuant that consists of a written work that has aesthetic value and artistic merit.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">literary work</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/LITERO_000017 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/LITERO_000017">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/LITERO_000016"/>
        <obo:IAO_0000115>A literary work in which the expression of feelings and ideas is given through style and the interplay of words and rhythm.</obo:IAO_0000115>
        <obo:comment>Poetry often employs rhyme and meter.</obo:comment>
        <rdfs:label xml:lang="en">poetry</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/LITERO_000018 -->

